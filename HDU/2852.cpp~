#include <cstdio>
#include <cstring>
#include <algorithm>
using namespace std;

const int maxn = 100000;
const int maxnode = (maxn << 5);

int n;
int sz, root[maxn + 10];
int lch[maxnode], rch[maxnode], sum[maxnode];

int main()
{
	while(scanf("%d", &n) == 1) {
		int cnt = 0;
		sz = 0;
		while(n--) {
			int op, a, b;
			scanf("%d%d", &op, &a);
			if(op == 0) {
				cnt++;
				root[cnt] = update(root[cnt-1], );
			}
		}
	}

	return 0;
}
