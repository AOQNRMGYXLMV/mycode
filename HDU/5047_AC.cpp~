#include <cstdio>

using namespace std ;

typedef long long ll ;

const ll mod=1000000 ;

int main()
{
    int T ;
    scanf("%d",&T) ;
    for(int cas=1 ;cas<=T ;cas++)
    {
        ll n ;
        scanf("%I64d",&n) ;
        //8*n*n-7*n+1
        ll t=8*n-7 ;
        ll lt=t/mod ;
        ll rt=t%mod ;
        lt*=n ;
        rt=rt*n+1 ;
        lt=lt+rt/mod ;
        rt%=mod ;
        printf("Case #%d: ",cas) ;
        if(lt)printf("%lld%06lld\n",lt,rt) ;
        else printf("%lld\n",rt) ;
    }
    return 0 ;
}
